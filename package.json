{
  "name": "larvitcss",
  "version": "0.6.0",
  "description": "Used to compile, minify, autoprefix etc CSS to remove the need to precompile css",
  "main": "index.js",
  "files": [
    "*.js",
    "controllers/**/*.js"
  ],
  "scripts": {
    "lint": "eslint *.js test/*.js",
    "test:unit": "tape ./test/*.js",
    "coverage": "nyc npm run test:unit",
    "test": "npm run lint && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larvit/larvitcss.git"
  },
  "keywords": [
    "css",
    "compile",
    "autocompile"
  ],
  "author": {
    "name": "Mikael 'Lilleman' GÃ¶ransson",
    "email": "lilleman@larvit.se",
    "url": "http://github.com/larvit/larvitcss"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/larvit/larvitcss/issues"
  },
  "homepage": "https://github.com/larvit/larvitcss#readme",
  "readmeFilename": "README.md",
  "dependencies": {
    "autoprefixer": "10.4.2",
    "postcss": "8.4.8",
    "sass": "1.49.9"
  },
  "private": false,
  "contributors": [],
  "devDependencies": {
    "axios": "0.26.1",
    "eslint": "8.11.0",
    "freeport": "1.0.5",
    "larvitbase-www": "0.8.0",
    "larvitutils": "5.0.0",
    "nyc": "15.1.0",
    "tape": "5.5.2"
  },
  "maintainers": [
    {
      "name": "lilleman",
      "email": "lilleman@larvit.se"
    }
  ]
}
